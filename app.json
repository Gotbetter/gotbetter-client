{
  "expo": {
    "name": "gotbetter-client",
    "slug": "gotbetter-client",
    "version": "0.0.3",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rpdyddns.gotbetter-client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 5,
      "package": "com.jjinyeok.gotbetterclient"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "769b0c09-83fb-43c6-a514-fe7fc34e10bc"
      }
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
